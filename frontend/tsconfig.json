{
  "compilerOptions": {
    "target": "es2022",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "include": [
    "new-types.d.ts",
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "public/maze.js",
    "../_unused/settings-mechant-fields.tsx",
    "../_unused/transferSuccessModalOLD.tsx",
    "../_unused/transferToBankModal.tsx",
    "../_unused/savings/savings.tsx",
    "../_unused/MoreOptionsModal.tsx",
    "../_unused/CbIntroModal.tsx"
  ],
  "exclude": ["node_modules"]
}
